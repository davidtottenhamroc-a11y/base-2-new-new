{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.js"
    },
    {
      "source": "/menu.html",
      "destination": "/api/auth-guard.js"
    },
    {
      "source": "/relatorios.html",
      "destination": "/api/auth-guard.js"
    },
    {
      "source": "/registro.html",
      "destination": "/api/auth-guard.js"
    }
  ]
}


